{
  "manifest_version": 3,

  "name": "Inter Font for csTimer",
  "description": "Changes text fonts in csTimer to Inter.",
  "version": "2",

  "content_scripts": [{
    "css": ["style.css"],
    "matches": ["https://*.cstimer.net/*", "http://*.cstimer.net/*"]
  }],

  "action": {
		"default_icon": "icon.png"
	}
}